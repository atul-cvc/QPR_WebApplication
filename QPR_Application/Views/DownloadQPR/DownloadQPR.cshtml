@model QPR_Application.Models.ViewModels.QPRReportViewModel

@{
    ViewData["Title"] = "DownloadQPR";
    Layout = null; //"~/Views/Shared/_UserLayout.cshtml";  // This disables the layout for this view
    string QtrAndYearInfo = "";
    if (ViewBag.IsAnnualReport)
        QtrAndYearInfo = "Year : " + ViewBag.ReportYear;
    else
        QtrAndYearInfo = ViewBag.QtrReport + ", " + ViewBag.ReportYear;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - QPR </title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/QPR_Application.styles.css" asp-append-version="true" />
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/JavaScript.js" asp-append-version="false"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="https://cdn.datatables.net/1.11.1/js/jquery.dataTables.min.js"></script>
    <link rel="stylesheet" href="https://cdn.datatables.net/1.11.1/css/jquery.dataTables.min.css" />
    <link href="~/lib/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
</head>
<body>

    <style>
        .form-control:disabled, .form-control[readonly] {
            background-color: white;
            opacity: 1;
        }
    </style>

    <div class="d-flex">
        <div class="justify-content-end ms-auto">
            <button class="btn btn-primary no-print" onclick="window.print(); return false;">Print</button>
        </div>
    </div>

    <center>
        <label>
            <h3 style="font-size:150%; ">
                QUARTERLY PERFORMANCE REPORT - @ViewBag.OrgName
                <br>
                @if (!string.IsNullOrEmpty(QtrAndYearInfo))
                {
                    <span>@QtrAndYearInfo</span>
                }
                <br>
                @if (!string.IsNullOrEmpty(@ViewBag.SubmissionDate) && !(ViewBag.IsAnnualReport))
                {
                    <span>
                        Date of Submission: @ViewBag.SubmissionDate
                    </span>
                }
            </h3>
        </label>
    </center>

