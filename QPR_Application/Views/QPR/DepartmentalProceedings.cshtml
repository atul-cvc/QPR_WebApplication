        <table class="table table-striped table-bordered w-100 text-align-middle table-blue table-padding">
            <tbody>
                <tr></tr>
                <tr class="text-center">
                    <th rowspan="2"><b>Category</b></th>
                    <th rowspan="2"><b>B/F from last quarter </b></th>
                    <th rowspan="2"><b>Inquiries assigned to IO during the quarter </b></th>
                    <th rowspan="2"><b>Total</b><br>4=(2+3)</th>
                    <th rowspan="2"><b>Reports received from IOs</b></th>
                    <th rowspan="2"><b>Inquiries Pending with IOs</b><br>6=(4-5)</th>
                    <th colspan="4"><b>Age wise Pendency(Months)<sup><span style="color: red;">**</span></sup> </b><br>6=(7+8+9+10)</th>
                </tr>
                <tr class="text-center">
                    <th>
                        &lt;6
                    </th>
                    <th>
                        6-12
                    </th>
                    <th>
                        12-18
                    </th>
                    <th>
                        &gt;18
                    </th>
                </tr>
                <tr class="text-center tr-grey">
                    <td><b>(1)</b></td>
                    <td><b>(2)</b></td>
                    <td><b>(3)</b></td>
                    <td><b>(4)</b></td>
                    <td><b>(5)</b></td>
                    <td><b>(6)</b></td>
                    <td><b>(7)</b></td>
                    <td><b>(8)</b></td>
                    <td><b>(9)</b></td>
                    <td><b>(10)</b></td>
                </tr>
                <tr>
                    <td align="left"> <b>&nbsp;&nbsp;Cases under CVC &nbsp;&nbsp;Jurisdiction</b> </td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_lastqtr" name="departproceedingsmajor_cvc_lastqtr" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_inquiries" name="departproceedingsmajor_cvc_inquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_total" name="departproceedingsmajor_cvc_total" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_reportsio" name="departproceedingsmajor_cvc_reportsio" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_enquiries" name="departproceedingsmajor_cvc_enquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_greatersix" name="departproceedingsmajor_cvc_greatersix" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_sixtotwelve" name="departproceedingsmajor_cvc_sixtotwelve" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_twelvetoeighteen" name="departproceedingsmajor_cvc_twelvetoeighteen" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_cvc_lesseighteen" name="departproceedingsmajor_cvc_lesseighteen" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
