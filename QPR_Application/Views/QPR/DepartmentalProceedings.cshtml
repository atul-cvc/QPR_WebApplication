                    <td><b><input asp-for="departproceedingsmajor_other_lesseighteen" name="departproceedingsmajor_other_lesseighteen" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                </tr>
                <tr>
                    <td align="left"><b>&nbsp;&nbsp;Total</b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_lastqtr" name="departproceedingsmajor_total_lastqtr" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_inquiries" name="departproceedingsmajor_total_inquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_total" name="departproceedingsmajor_total_total" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_reportsio" name="departproceedingsmajor_total_reportsio" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_enquiries" name="departproceedingsmajor_total_enquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_greatersix" name="departproceedingsmajor_total_greatersix" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_total_sixtotwelve" name="departproceedingsmajor_total_sixtotwelve" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
