                <tr>
                    <td align="left"><b>&nbsp;&nbsp;Other (non-CVC &nbsp;&nbsp;cases)</b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_lastqtr" name="departproceedingsmajor_other_lastqtr" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_inquiries" name="departproceedingsmajor_other_inquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_total" name="departproceedingsmajor_other_total" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_reportsio" name="departproceedingsmajor_other_reportsio" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_enquiries" name="departproceedingsmajor_other_enquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_greatersix" name="departproceedingsmajor_other_greatersix" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedingsmajor_other_sixtotwelve" name="departproceedingsmajor_other_sixtotwelve" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
