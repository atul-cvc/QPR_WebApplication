        <table class="table table-striped table-bordered w-100 text-align-middle table-blue table-padding">

            <tbody>
                <tr></tr>
                <tr class="text-center">
                    <th rowspan="2"><b>Category</b></th>
                    <th rowspan="2"><b>B/F from last quarter </b></th>
                    <th rowspan="2"><b>Minor penalty charge sheet by DAs during the quarter </b></th>
                    <th rowspan="2"><b>Total</b><br>4=(2+3)</th>
                    <th rowspan="2"><b>Cases in which Final Orders issue by DAs</b></th>
                    <th rowspan="2"><b>Balance pending</b><br>6=(4-5)</th>
                    <th colspan="4"><b>Age wise Pendency(Months)<sup><span style="color: red;">**</span></sup> </b><br>6=(7+8+9+10)</th>
                </tr>
                <tr class="text-center">
                    <th>
                        &lt;6 months
                    </th>
                    <th>
                        6-12 months
                    </th>
                    <th>
                        12-18 months
                    </th>
                    <th>
                        &gt;18 months
                    </th>
                </tr>
                <tr class="text-center tr-grey" >
                    <td>&nbsp;<b>(1)</b></td>
                    <td>&nbsp;<b>(2)</b></td>
                    <td>&nbsp;<b>(3)</b></td>
                    <td>&nbsp;<b>(4)</b></td>
                    <td>&nbsp;<b>(5)</b></td>
                    <td>&nbsp;<b>(6)</b></td>
                    <td>&nbsp;<b>(7)</b></td>
                    <td>&nbsp;<b>(8)</b></td>
                    <td>&nbsp;<b>(9)</b></td>
                    <td>&nbsp;<b>(10)</b></td>
                </tr>
                <tr>
                    <td align="left"> <b>&nbsp;&nbsp;Cases under CVC &nbsp;&nbsp;Jurisdiction</b> </td>
                    <td><b><input asp-for="departproceedings_minor_cvc_lastqtr" name="departproceedings_minor_cvc_lastqtr" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedings_minor_cvc_inquiries" name="departproceedings_minor_cvc_inquiries" onkeypress="return isNumberKey(event);" class="form-control"></b></td>
                    <td><b><input asp-for="departproceedings_minor_cvc_total" name="departproceedings_minor_cvc_total" onkeypress="return isNumberKey(event);" class="form-control" ></b></td>
                    <td><b><input asp-for="departproceedings_minor_cvc_reportsio" name="departproceedings_minor_cvc_reportsio" onkeypress="return isNumberKey(event);" class="form-control" ></b></td>
                    <td><b><input asp-for="departproceedings_minor_cvc_enquiries" name="departproceedings_minor_cvc_enquiries" onkeypress="return isNumberKey(event);" class="form-control" ></b></td>
