@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

<script>
    document.getElementById('add-row-dept-proc').addEventListener('click', function () {
        $('#NewAgainstChargedTable_departproceedings_detailsinquiry_chargedofficer').attr('required', true);
        $('#NewAgainstChargedTable_departproceedings_detailsinquiry_remarks').attr('required', true);
        $('#newRow').removeClass('d-none');
        $('#add-row-dept-proc').removeClass('btn-outline-primary');
        $('#add-row-dept-proc').addClass('btn-outline-secondary');
        $('#add-row-dept-proc').addClass('disabled');
    });

    function hideRow() {
        event.preventDefault();
        $('#NewAgainstChargedTable_departproceedings_detailsinquiry_chargedofficer').removeAttr('required');
        $('#NewAgainstChargedTable_departproceedings_detailsinquiry_remarks').removeAttr('required');
        $('#newRow').addClass('d-none');
        $('#add-row-dept-proc').addClass('btn-outline-primary');
        $('#add-row-dept-proc').removeClass('btn-outline-secondary');
        $('#add-row-dept-proc').removeClass('disabled');
    }
</script>

<script>
    $(function () {
        var complaintsMessage = '@ViewBag.ComplaintsMessage';
        if (complaintsMessage) {
            Swal.fire({
                // title: complaintsMessage,
                text: complaintsMessage,
                icon: 'success',
                confirmButtonText: 'Okay'
            }).then((result) => {
                if (result.isConfirmed) {
                    pageReloadWithoutQueryString();
                } else {
                    // Optional: Handle cancel action
                }
            });
        }
        performComTotal();
    });

    $('#Departmentalproceedingsqrs_departproceedings_minor_cvc_lesseighteen, #Departmentalproceedingsqrs_departproceedings_minor_other_lesseighteen,#Departmentalproceedingsqrs_departproceedings_minor_cvc_twelvetoeighteen,#Departmentalproceedingsqrs_departproceedings_minor_other_twelvetoeighteen,#Departmentalproceedingsqrs_departproceedings_minor_cvc_sixtotwelve,#Departmentalproceedingsqrs_departproceedings_minor_other_sixtotwelve,#Departmentalproceedingsqrs_departproceedings_minor_other_greatersix,#Departmentalproceedingsqrs_departproceedings_minor_cvc_greatersix,#Departmentalproceedingsqrs_departproceedings_minor_other_reportsio, #Departmentalproceedingsqrs_departproceedings_minor_cvc_reportsio,#Departmentalproceedingsqrs_departproceedings_minor_other_inquiries, #Departmentalproceedingsqrs_departproceedings_minor_cvc_inquiries,#Departmentalproceedingsqrs_departproceedingsmajor_other_reportsio, #Departmentalproceedingsqrs_departproceedingsmajor_other_lesseighteen, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_lesseighteen, #Departmentalproceedingsqrs_departproceedingsmajor_other_twelvetoeighteen, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_twelvetoeighteen, #Departmentalproceedingsqrs_departproceedingsmajor_other_sixtotwelve, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_sixtotwelve, #Departmentalproceedingsqrs_departproceedingsmajor_other_greatersix, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_greatersix, #departproceedingsmajor_other_reportsio, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_reportsio, #Departmentalproceedingsqrs_departproceedingsmajor_other_inquiries, #Departmentalproceedingsqrs_departproceedingsmajor_cvc_inquiries ').on('input', performComTotal);

    function performComTotal() {
        $('#Departmentalproceedingsqrs_departproceedingsmajor_cvc_total').val((parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_cvc_inquiries').val()) || 0) + (parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_cvc_lastqtr').val()) || 0));
        $('#Departmentalproceedingsqrs_departproceedingsmajor_other_total').val((parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_other_inquiries').val()) || 0) + (parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_other_lastqtr').val()) || 0));
        $('#Departmentalproceedingsqrs_departproceedingsmajor_total_lastqtr').val((parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_other_lastqtr').val()) || 0) + (parseFloat($('#Departmentalproceedingsqrs_departproceedingsmajor_cvc_lastqtr').val()) || 0));
