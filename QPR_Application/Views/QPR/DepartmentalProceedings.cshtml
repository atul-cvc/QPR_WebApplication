@model QPR_Application.Models.Entities.departmentalproceedingsqrs

@{
    ViewData["Title"] = "DepartmentalProceedings";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<div class="text-center">
    <h3>Departmental Proceedings</h3>
</div>
<hr />
<form asp-action="DepartmentalProceedings">
    <div class="w-80 mt-5 mb-3 box-green ">
        <b>
            4.Departmental Inquiries (Nos)
        </b>
    </div>

    <div class="w-80 mt-2 mb-3 box-green ">
        <b>
            4.(A) Major Penalty* cases pending with IOs/IAs
        </b>
    </div>


