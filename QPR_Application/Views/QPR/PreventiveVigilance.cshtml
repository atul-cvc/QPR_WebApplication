        else if (totRowsSecond < detail_vig_machanism_b) {
            $("#totRowsSecond").val(detail_vig_machanism_b);
            detail_vig_machanism_b = parseInt(detail_vig_machanism_b) - parseInt(totRowsSecond)
        } else {
            alert("Total number of officer can not be less than already selected number of officers");
            return;
        }

        cntCall1++;
