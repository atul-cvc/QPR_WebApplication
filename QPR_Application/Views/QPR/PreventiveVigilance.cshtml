<script>
    var cnt = -1;
    var cntCall = 0;

    $(".add-row-preventive").click(function () {
        // debugger;
        var detail_vig_machanism_a = parseInt($("#detail_vig_machanism_a").val());
        var totRows = parseInt($("#totRows-preventive").val());

        if (totRows == 0) {
            $("#totRows-preventive").val(detail_vig_machanism_a);
        }
        else if (totRows < detail_vig_machanism_a) {
            $("#totRows-preventive").val(detail_vig_machanism_a);
            detail_vig_machanism_a = parseInt(detail_vig_machanism_a) - parseInt(totRows)
        } else {
            alert("Total number of officer can not be less than already selected number of officers");
            return;
        }


        cntCall++;
        var markup = "";
        for (var i = 0; i < parseInt(detail_vig_machanism_a); i++) {

            markup = "<tr><td style='width:1%'>" + (cnt++) + "</td><td style='width:8%'><input class='form-control' name='VigiA[" + (cnt) + "].preventivevigi_management_detailsvig_a_nameofsub'  /></td>";
            markup = markup + "<td style='width:8%'><input type='text' class='form-control' name='VigiA[" + (cnt) + "].preventivevigi_management_detailsvig_a_staff_appointed'  ></td>";
            markup = markup + "<td style='width:18%'><input type='text' class='form-control' name='VigiA[" + (cnt) + "].preventivevigi_management_detailsvig_a_methodofcontrol' ><input type='hidden' name='VigiA[" + (cnt) + "].preventiveVigiA_Id' ></td>";
            markup = markup + "<td style='width:3%'>" + "<a href='javascript:void(0)'  onclick='return removeDisplayFunction(this);' class='delete-row btn btn-outline-danger empty' >Delete</a></td></tr>";

            $("#officerNumber-preventive tbody").append(markup);


        }
        resetSerialNumberPreventive();
    });

    function resetSerialNumberPreventive() {
        var sno = 1;
        $("#officerNumber-preventive tbody tr").each(function (i) {

            // find the first td in the row
            $(this).find("td:first").text(sno++);

        });
    }

    function removeDisplayFunction(btndel, pendId) {
        // debugger;
        // var class = btndel.attr('class')
        var classes = btndel.className;
        if (!classes.includes('empty') && pendId != 0) {
            $.ajax({
                type: "POST",
                url: "/pro11/user/cvofficer/deleteRowVigiA",
                data: {
                    "pendId": pendId
                },
                success: function (data) {
                    alert(data);
                    console.log("SUCCESS: ", data);
                },
                error: function (e) {
                    console.log("ERROR: ", e);

                },
                done: function (e) {
                    console.log("DONE");
                }
            });
        }
    }

    $(".RowsSecond").click(function () {
        var detail_vig_machanism_b = parseInt($("#detail_vig_machanism_b").val());
        var totRowsSecond = parseInt($("#totRowsSecond").val());

