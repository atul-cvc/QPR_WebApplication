            markup = "<tr><td style='width:1%'>" + (cnt1++) + "</td><td style='width:8%'><input clas path='' name='VigiB[" + (cnt1) + "].preventivevigi_management_detailsvig_b_nameofsub' style='width:99%' /></td>";
            markup = markup + "<td style='width:8%'><input path=''  name='VigiB[" + (cnt1) + "].preventivevigi_management_detailsvig_b_contral_method'  style='width:99%'></td>";
            markup = markup + "<td style='width:8%'><input path=''  name='VigiB[" + (cnt1) + "].preventivevigi_management_detailsvig_b_likely_time' style='width:99%'><input path='' type='hidden'  name='VigiB[" + (cnt1) + "].preventiveVigiB_Id' style='width:99%'></td>";
