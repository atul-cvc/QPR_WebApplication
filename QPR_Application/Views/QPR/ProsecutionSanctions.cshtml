                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingcbifirno</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingnamedesig</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingdaterecommend</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingdatereceipt</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingsanctionpc</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingstatusrequest</span>
                                        </td>
                                        <td>
                                            <label class="control-label"></label>
                                            <span>@Model.Agewisependency[i].prosependingnameauthority</span>
                                        </td>
                                        <td>
                                            <a href="" class="btn btn-outline-danger" onclick="confirmDel('@Url.Action("DeleteAgeWisePendency", new { pend_id = @Model.Agewisependency[@i].pend_id })'); return false;">Delete</a>
                                        </td>
                                    </tr>
                                }
                                <tr class="text-center d-none" id="newAgeWiseRow">
                                    <td>@(Model.Agewisependency.Count + 1)</td> <!-- Serial number column -->
                                    <td>
                                        <input asp-for='NewAgewisependency.prosependingcbifirno' class='form-control' type='text' />
                                    </td>
                                    <td>
                                        <input asp-for='NewAgewisependency.prosependingnamedesig' class='form-control' type='text' />
                                    </td>
                                    <td>
                                        <input asp-for='NewAgewisependency.prosependingdaterecommend' class='form-control ' type='date' />
                                    </td>
                                    <td>
                                        <input asp-for='NewAgewisependency.prosependingdatereceipt' class='form-control ' type='date' />
                                    </td>

                                    <td>
                                        <input asp-for='NewAgewisependency.prosependingsanctionpc' class='form-control' type='text' />

