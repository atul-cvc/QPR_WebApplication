    <div class="d-flex justify-content-between w-80 m-auto mt-5">
        <a class="btn btn-secondary" asp-action="VigilanceInvestigation">Back</a>
        <input type="submit" value="Save" class="btn btn-success" asp-action="SaveProsecutionSanctions" onclick="return VERIFY_SAVE_PROSANC()" />
        <input type="submit" value="Next" class="btn btn-primary " onclick="return VERIFY_NEXT_PROSANC()" />
