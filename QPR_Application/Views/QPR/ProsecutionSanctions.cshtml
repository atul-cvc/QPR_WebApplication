</form>




@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }

    <script>
        document.getElementById('add-row-prosec').addEventListener('click', function () {
            //
            $('#NewAgewisependency_prosependingcbifirno').attr('required', true);
            $('#NewAgewisependency_prosependingnamedesig').attr('required', true);
            $('#NewAgewisependency_prosependingsanctionpc').attr('required', true);
            $('#NewAgewisependency_prosependingstatusrequest').attr('required', true);
            $('#NewAgewisependency_prosependingnameauthority').attr('required', true);
            $('#newAgeWiseRow').removeClass('d-none');
            $('#add-row-prosec').removeClass('btn-outline-primary');
            $('#add-row-prosec').addClass('btn-outline-secondary');
            $('#add-row-prosec').addClass('disabled');

        });

        function hideRow() {
            event.preventDefault();
            $('#NewAgewisependency_prosependingcbifirno').removeAttr('required');
            $('#NewAgewisependency_prosependingnamedesig').removeAttr('required');
            $('#NewAgewisependency_prosependingsanctionpc').removeAttr('required');
            $('#NewAgewisependency_prosependingstatusrequest').removeAttr('required');
            $('#NewAgewisependency_prosependingnameauthority').removeAttr('required');
            $('#newAgeWiseRow').addClass('d-none');
            $('#add-row-prosec').addClass('btn-outline-primary');
            $('#add-row-prosec').removeClass('btn-outline-secondary');
            $('#add-row-prosec').removeClass('disabled');

        };

    </script>

    <script>

        $(function () {
            var complaintsMessage = '@ViewBag.ComplaintsMessage';
            if (complaintsMessage) {
                var notificationIcon;

                if (complaintsMessage == 'Saved' || complaintsMessage == 'Deleted successfully') {
                    notificationIcon = 'success'
                }

                if (complaintsMessage.includes('Error')) {
                    notificationIcon = 'error'
                }

                Swal.fire({
                    // title: complaintsMessage,
                    text: complaintsMessage,
                    icon: notificationIcon,
                    confirmButtonText: 'Okay'
                }).then((result) => {
                    if (result.isConfirmed) {
                        pageReloadWithoutQueryString();
                    } else {
                        // Optional: Handle cancel action
                    }
                });
            }
            performComTotal();
        });

        $('#Prosecutionsanctionsqrs_prosevigisuspensionqtr, #Prosecutionsanctionsqrs_prosevigisuspensionordered, #Prosecutionsanctionsqrs_Prosecutionsanctionsqrs_prosesanctjslesssix,#Prosecutionsanctionsqrs_prosesanctgroupalesssix,#Prosecutionsanctionsqrs_prosesanctgroupblesssix,#Prosecutionsanctionsqrs_prosesanctgroupclesssix, #Prosecutionsanctionsqrs_prosesanctjsthreetosix,#Prosecutionsanctionsqrs_prosesanctgroupathreetosix,#Prosecutionsanctionsqrs_prosesanctgroupcthreetosix,#Prosecutionsanctionsqrs_prosesanctgroupbthreetosix,#Prosecutionsanctionsqrs_prosesanctjsgreaterthree, #Prosecutionsanctionsqrs_prosesanctgroupagreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupbgreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupcgreaterthree,#Prosecutionsanctionsqrs_prosesanctjsgreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupbbalancepending, #Prosecutionsanctionsqrs_prosesanctgroupcbalancepending,#Prosecutionsanctionsqrs_prosesanctjsbalancepending,#Prosecutionsanctionsqrs_prosesanctgroupabalancepending,#Prosecutionsanctionsqrs_prosesanctgroupasanctionrefused,#Prosecutionsanctionsqrs_prosesanctjssanctionrefused, #Prosecutionsanctionsqrs_prosesanctjssanctiongranted, #Prosecutionsanctionsqrs_prosesanctgroupasanctiongranted,#Prosecutionsanctionsqrs_prosesanctgroupbsanctionrefused, #Prosecutionsanctionsqrs_prosesanctgroupbsanctiongranted, #Prosecutionsanctionsqrs_prosesanctgroupcsanctionrefused, #Prosecutionsanctionsqrs_prosesanctgroupcsanctiongranted, #Prosecutionsanctionsqrs_prosesanctjsreciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupareciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').on('input', performComTotal);


        function performComTotal() {

            $('#Prosecutionsanctionsqrs_prosesancttotalopeningbalance ').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupaopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcopeningbalance').val()) || 0));
            $('#Prosecutionsanctionsqrs_prosesancttotalreciveqtr').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupareciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').val()) || 0));

            $('#Prosecutionsanctionsqrs_prosesanctgroupctotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcopeningbalance').val()) || 0));
            $('#Prosecutionsanctionsqrs_prosesanctgroupbtotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbopeningbalance').val()) || 0));
