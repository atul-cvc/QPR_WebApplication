    </div>
    <div class="center w-70 mt-2">
        <p><sup><span style="color: red;">*</span></sup> Include number of officers falling under CVC jurisdiction- Category A officers only.</p>
    </div>

    <div class="w-25 m-auto">
        @if (ViewBag.ComplaintsMessage != null)
        {
            @ViewBag.ComplaintsMessage
        }
    </div>

    <div class="d-flex justify-content-between w-80 m-auto mt-5">
        <a class="btn btn-secondary" asp-action="VigilanceInvestigation">Back</a>
        <input type="submit" value="Save" class="btn btn-success" asp-action="SaveProsecutionSanctions" />
        <input type="submit" value="Next" class="btn btn-primary " />
    </div>


</form>



@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}

<script>
    $(function () {
        var complaintsMessage = '@ViewBag.ComplaintsMessage';
        if (complaintsMessage) {
            alert(complaintsMessage);
            pageReloadWithoutQueryString();
        }
        performComTotal();
    });

    $('#Prosecutionsanctionsqrs_Prosecutionsanctionsqrs_prosesanctjslesssix,#Prosecutionsanctionsqrs_prosesanctgroupalesssix,#Prosecutionsanctionsqrs_prosesanctgroupblesssix,#Prosecutionsanctionsqrs_prosesanctgroupclesssix, #Prosecutionsanctionsqrs_prosesanctjsthreetosix,#Prosecutionsanctionsqrs_prosesanctgroupathreetosix,#Prosecutionsanctionsqrs_prosesanctgroupcthreetosix,#Prosecutionsanctionsqrs_prosesanctgroupbthreetosix,#Prosecutionsanctionsqrs_prosesanctjsgreaterthree, #Prosecutionsanctionsqrs_prosesanctgroupagreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupbgreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupcgreaterthree,#Prosecutionsanctionsqrs_prosesanctjsgreaterthree,#Prosecutionsanctionsqrs_prosesanctgroupbbalancepending, #Prosecutionsanctionsqrs_prosesanctgroupcbalancepending,#Prosecutionsanctionsqrs_prosesanctjsbalancepending,#Prosecutionsanctionsqrs_prosesanctgroupabalancepending,#Prosecutionsanctionsqrs_prosesanctgroupasanctionrefused,#Prosecutionsanctionsqrs_prosesanctjssanctionrefused, #Prosecutionsanctionsqrs_prosesanctjssanctiongranted, #Prosecutionsanctionsqrs_prosesanctgroupasanctiongranted,#Prosecutionsanctionsqrs_prosesanctgroupbsanctionrefused, #Prosecutionsanctionsqrs_prosesanctgroupbsanctiongranted, #Prosecutionsanctionsqrs_prosesanctgroupcsanctionrefused, #Prosecutionsanctionsqrs_prosesanctgroupcsanctiongranted, #Prosecutionsanctionsqrs_prosesanctjsreciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupareciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr ,#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').on('input', performComTotal);


    function performComTotal() {

        $('#Prosecutionsanctionsqrs_prosesancttotalopeningbalance ').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupaopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbopeningbalance').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcopeningbalance').val()) || 0));
        $('#Prosecutionsanctionsqrs_prosesancttotalreciveqtr').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupareciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').val()) || 0));

        $('#Prosecutionsanctionsqrs_prosesanctgroupctotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcopeningbalance').val()) || 0));
        $('#Prosecutionsanctionsqrs_prosesanctgroupbtotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbopeningbalance').val()) || 0));
        $('#Prosecutionsanctionsqrs_prosesanctgroupatotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupareciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupaopeningbalance').val()) || 0));
        $('#Prosecutionsanctionsqrs_prosesanctjstotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsreciveqtr').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctjsopeningbalance').val()) || 0));

        $('#Prosecutionsanctionsqrs_prosesancttotal').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjstotal').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupatotal').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbtotal').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupctotal').val()) || 0));


        $('#Prosecutionsanctionsqrs_prosesancttotalsanctiongranted').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjssanctiongranted').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupasanctiongranted').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbsanctiongranted').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcsanctiongranted').val()) || 0));
        $('#Prosecutionsanctionsqrs_prosesancttotalsanctionrefused').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctjssanctionrefused').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupasanctionrefused').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbsanctionrefused').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcsanctionrefused').val()) || 0));

        $('#Prosecutionsanctionsqrs_prosesanctgroupcbalancepending').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupctotal').val()) || 0) - ((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcsanctiongranted').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupcsanctionrefused').val()) || 0)));
        $('#Prosecutionsanctionsqrs_prosesanctgroupbbalancepending').val((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbtotal').val()) || 0) - ((parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbsanctiongranted').val()) || 0) + (parseFloat($('#Prosecutionsanctionsqrs_prosesanctgroupbsanctionrefused').val()) || 0)));
