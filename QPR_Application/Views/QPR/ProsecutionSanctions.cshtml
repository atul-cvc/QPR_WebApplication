        <table class="table table-striped table-bordered w-100 text-align-middle table-blue table-padding">
            <tbody>
                <tr class="text-center">
                    <th rowspan="2"><b>Nos. of officers under suspension at the beginning of quarter</b></th>
                    <th rowspan="2"><b>Suspensions ordered during the quarter</b></th>
                    <th rowspan="2"><b>Total</b><br>3=(1+2)</th>
                    <th rowspan="2"><b>Suspension revoked during the quarter</b></th>
                    <th rowspan="2"><b>Nos. of officers under suspension at the end of quarter</b><br>5=(3 - 4)</th>

                </tr>
                <tr></tr>
                <tr class="text-center tr-grey">
                    <td>&nbsp;<b>(1)</b></td>
                    <td>&nbsp;<b>(2)</b></td>
                    <td>&nbsp;<b>(3)</b></td>
                    <td>&nbsp;<b>(4)</b></td>
                    <td>&nbsp;<b>(5)</b></td>
                </tr>
                <tr>
                    <td>
                        <b>
                            <input asp-for="Prosecutionsanctionsqrs.prosevigiofficersuspension" class="form-control" />
