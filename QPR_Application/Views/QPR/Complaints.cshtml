@model QPR_Application.Models.Entities.complaintsqrs

@{
    ViewData["Title"] = "Complaints";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
<style>
  /*   table td {
        padding: 0.25em !important;
        vertical-align: middle !important;
    } */
</style>


<div>
    <h3 class="text-center">Complaints</h3>
</div>
<hr />

<form asp-action="Complaints">
    <div class="w-75 m-auto">
        <table class="table table-striped table-bordered table-blue table-padding">
            <tbody>
                <tr class="text-center">
                    <th rowspan="2"><b>Source</b></th>
                    <th rowspan="2"><b>Opening Balance</b></th>
                    <th rowspan="2"><b>Received During the Quarter</b></th>
                    <th rowspan="2"><b>Total<br>4=(2+3)</b></th>
                    <th rowspan="2" style="width:22%;"><b>Disposal During The Quarter</b><br>(Filed/Sent for NA and Registered as Vigilance for Investigation by CVO)</th>
