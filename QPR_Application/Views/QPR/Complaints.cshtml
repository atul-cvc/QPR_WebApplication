@model QPR_Application.Models.Entities.complaintsqrs

@{
    ViewData["Title"] = "Complaints";
    Layout = "~/Views/Shared/_UserLayout.cshtml";
}
<style>
    table th {
        background-color: rgb(77,109,244) !important;
        color: white;
        font-weight: 300;
        font-size: 18px;
    }

    table td {
        padding: 0.25em !important;
        /* text-align:center !important; */
        vertical-align: middle !important;
    }

</style>

<div>
    <h1 class="text-center">Complaints</h1>
</div>

@* <h4>complaintsqrs</h4> *@
<hr />

<form asp-action="Complaints">
