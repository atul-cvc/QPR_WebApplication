        console.log('totnotother triggered')
        var tot = 0;
        tot += (parseFloat($("#complaints_signed_complaints").val()) || 0);
        tot += (parseFloat($("#complaints_department").val()) || 0);
        tot += (parseFloat($("#complaints_cbi").val()) || 0);
