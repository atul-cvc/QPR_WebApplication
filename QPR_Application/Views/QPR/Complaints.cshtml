                            <input asp-for="comcvcbalance_pending" class="form-control" />
                        </b>
                    </td>
                    <td>
                        <b>
                            @* <input id="comCVCage_less_one" name="comCVCage_less_one" onkeypress="return isNumberKey(event);" style="padding: 2px;" type="text" class="comGreaterOne bPTotalCVC" value="0"> *@
