        tot += (parseFloat($("#complaints_audit").val()) || 0);
        tot += (parseFloat($("#complaints_detected").val()) || 0);

        var recv = $("#comotherreceived_during_quarter").val();
        var msg = `Total of "1.1 Break Up of Complaints Received Under Source OTHERS During The Quarter" must be equal to: ${recv}`;

        if (parseFloat($("#comotherreceived_during_quarter").val()) < tot) {
            Swal.fire({
                text: `${msg}`,
                icon: 'info',
                confirmButtonText: 'Okay'
