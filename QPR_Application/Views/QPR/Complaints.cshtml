    function verifySumQTY1() {
        var pend = 0;
        pend = parseFloat($("#comcvctotal").val()) - parseFloat($("#comcvcdisposal_during_quarter").val());
        // debugger;
        if (pend < 0) {
            $("#comcvcdisposal_during_quarter").val(0);
            $("#comcvcage_less_one").val(0);
            $("#comcvcage_pendency_betweenone_three").val(0);
            $("#comcvcage_pendency_betweenThree_Six").val(0);
        }
        performComTotal();
    }

    function verifySumQTY2() {
        var pend = 0;
        pend = parseFloat($("#comotherreceived_during_quarter").val()) - parseFloat($("#comotherdisposal_during_quarter").val());
        if (pend < 0) {
            $("#comotherdisposal_during_quarter").val(0);
            $("#comotherage_less_one").val(0);
            $("#comotherage_pendency_betweenone_three").val(0);
            $("#comotherage_pendency_betweenthree_six").val(0);
        }
        performComTotal();
    }

    function totNotGreater1() {
        if (parseFloat($("#comcvcdisposal_during_quarter").val()) > parseFloat($("#comcvctotal").val())) {
            Swal.fire({
                text: `Disposal cannot be greater than total`,
                icon: 'info',
                confirmButtonText: 'Okay'
            });
            $("#comcvcdisposal_during_quarter").val(0);
        }
        performComTotal();
    }

    function totNotGreater2() {
        if (parseFloat($("#comotherdisposal_during_quarter").val()) > parseFloat($("#comothertotal").val())) {
            Swal.fire({
                text: `Disposal cannot be greater than total`,
                icon: 'info',
                confirmButtonText: 'Okay'
            });
            $("#comotherdisposal_during_quarter").val(0);
            performComTotal();
        }
    }

