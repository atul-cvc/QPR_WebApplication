            <tbody>
                <tr>
                    <td style=" text-align: left;"><b>&nbsp;&nbsp;(a) Signed Complaints recieved by CVO</b></td>
                    <td>
                        <b>
                            @* <input id="complaints_Signed_Complaints" name="complaints_Signed_Complaints" onkeypress="return isNumberKey(event);" style=" padding:2px;" type="text" class="signedComplaints" value="0"> *@
                            <input asp-for="complaints_signed_complaints" class="form-control" />

                        </b>
                    </td>
                </tr>
                <tr>
                    <td style=" text-align: left;"><b>&nbsp;&nbsp;(b) Complaints from Ministry / Department</b></td>
                    <td>
                        <b>
                            @* <input id="complaints_department" name="complaints_department" onkeypress="return isNumberKey(event);" style=" padding:2px;" type="text" class="signedComplaints" value="0"> *@
                            <input asp-for="complaints_department" class="form-control" />
                        </b>
                    </td>
                </tr>
                <tr>
                    <td style=" text-align: left;"><b>&nbsp;&nbsp;(c) Complaints from CBI </b></td>
