                            <input type="number" asp-for="comcvcopeningbalance" class="form-control" />
                        </b>
                    </td>
                    <td>
                        <b>
                            @* <input id="ComCVCReceived_during_quarter" name="ComCVCReceived_during_quarter" onkeypress="return isNumberKey(event);" style="padding: 2px;" type="text" class="qty1 comRec" value="0"> *@
                            <input type="number" asp-for="comcvcreceived_during_quarter" class="form-control" />
                        </b>
                    </td>

                    <td>
                        <b>
                            @* <input id="comCVCTotal" name="comCVCTotal" onkeypress="return isNumberKey(event);" style="padding: 2px;" readonly="true" type="text" class="total comtotal subbptotal" value="0"> *@
                            <input asp-for="comcvctotal" class="form-control" />
