@model IEnumerable<QPR_Application.Models.Entities.qpr>

@{
    ViewData["Title"] = "QPR";
}

<h1>QPR</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<div class="table-responsive">
    <table id="example1" class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.userid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.orgname)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.orgcode)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.qtrreport)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.qtryear)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.contactnumberoffice)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.mobilenumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.emailid)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.fulltime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.parttime)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.createdate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ip)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.finalsubmit)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.finalsubmitdate)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.userid)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.orgname)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.orgcode)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.qtrreport)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.qtryear)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.contactnumberoffice)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.mobilenumber)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.emailid)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.fulltime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.parttime)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.createdate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ip)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.finalsubmit)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.finalsubmitdate)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.referencenumber">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.referencenumber">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.referencenumber">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

@section Scripts {
    <script>
        $(document).ready(function () {
            $('#example1').DataTable();
        });
    </script>
}
